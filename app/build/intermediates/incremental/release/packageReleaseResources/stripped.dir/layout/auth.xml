<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical"
	android:id="@+id/_main">
	<ScrollView
		android:id="@+id/vscroll1"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="@color/md_theme_background"
		android:scrollbars="none">
		<LinearLayout
			android:id="@+id/parentLayout"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:paddingLeft="18dp"
			android:paddingTop="46dp"
			android:paddingRight="18dp"
			android:paddingBottom="18dp"
			android:background="@android:color/transparent"
			android:orientation="vertical"
			android:animateLayoutChanges="true">
			<com.synapse.social.studioasinc.animations.textview.TVeffects
				android:id="@+id/aiNameTextView"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="Hello, I'm Synapse AI"
				android:textSize="30sp"
				android:textStyle="bold"
				android:textColor="@color/md_theme_onBackground"
				android:fontFamily="@font/product_sans_bold" />
			<com.synapse.social.studioasinc.animations.textview.TVeffects
				android:id="@+id/aiResponseTextView_1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="15dp"
				android:textSize="18sp"
				android:textColor="#000000"
				android:fontFamily="@font/product_sans" />
			<LinearLayout
				android:id="@+id/mainHiddenLayout"
				android:layout_width="match_parent"
				android:layout_height="300dp"
				android:layout_marginTop="15dp"
				android:gravity="center_horizontal"
				android:orientation="vertical"
				android:visibility="gone"
				android:animateLayoutChanges="true">
				<LinearLayout
					android:id="@+id/nameLayout"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="16dp"
					android:orientation="vertical"
					android:background="@drawable/white_rounded_stroke"
					android:animateLayoutChanges="true">
					<LinearLayout
						android:id="@+id/profileSeparatorLayout"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="horizontal"
						android:animateLayoutChanges="true">
						<LinearLayout
							android:id="@+id/profileHolderLayout"
							android:layout_width="wrap_content"
							android:layout_height="match_parent"
							android:layout_marginRight="10dp"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical"
							android:visibility="gone"
							android:animateLayoutChanges="true">
							<LinearLayout
								android:id="@+id/latterProfilePictureLayoutCircle"
								android:layout_width="50dp"
								android:layout_height="50dp"
								android:gravity="center_horizontal|center_vertical"
								android:orientation="vertical"
								android:background="@drawable/circlelayout">
								<TextView
									android:id="@+id/nameFirstLetterTextView"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:text="A"
									android:textSize="20sp"
									android:textStyle="bold"
									android:textColor="?colorSurface" />
							</LinearLayout>
						</LinearLayout>
						<LinearLayout
							android:id="@+id/nameLayoutHolderLayout"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:orientation="vertical">
							<TextView
								android:id="@+id/userAutoResponseTextView_1"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="Nice to meet you, I'm..."
								android:textSize="16sp"
								android:textColor="#000000"
								android:fontFamily="@font/product_sans" />
							<LinearLayout
								android:id="@+id/EditTextContainerLayout"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:layout_marginTop="5dp"
								android:background="@android:color/transparent"
								android:orientation="horizontal">
								<EditText
									android:id="@+id/usernameEditText"
									android:layout_width="match_parent"
									android:layout_height="match_parent"
									android:padding="2dp"
									android:background="@android:color/transparent"
									android:gravity="center_vertical"
									android:textSize="18sp"
									android:textColor="#000000"
									android:hint="Enter your name"
									android:textColorHint="#9E9E9E"
									android:singleLine="true"
									android:fontFamily="@font/product_sans" />
							</LinearLayout>
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/animatorSupportLayout"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:orientation="vertical"
					android:layout_weight="1" />
				<LinearLayout
					android:id="@+id/linear2"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="10dp"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/ageCheckboxLayout"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="horizontal"
						android:background="@drawable/rounded_stroke">
						<CheckBox
							android:id="@+id/confirmAgeCheckBox"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginLeft="12dp"
							android:layout_marginRight="12dp"
							android:background="@android:color/transparent"
							android:gravity="center_vertical"
							android:text="@string/age_agreement"
							android:textSize="12sp"
							android:textStyle="bold"
							android:textColor="?colorOnSurface"
							android:fontFamily="@font/product_sans" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/continueBtnHolderLayout"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginTop="15dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<Button
							android:id="@+id/continueButton"
							android:layout_width="match_parent"
							android:layout_height="60dp"
							android:padding="8dp"
							android:text="Continue"
							android:textSize="16sp"
							android:textColor="?colorSurface"
							style="@style/Widget.Material3.Button"
							android:fontFamily="@font/product_sans_bold" />
					</LinearLayout>
					<TextView
						android:id="@+id/infoTextView_1"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginTop="15dp"
						android:gravity="center_horizontal|center_vertical"
						android:text="You can always change your name later"
						android:textSize="12sp"
						android:textColor="@color/md_theme_onBackground"
						android:fontFamily="@font/product_sans" />
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/section2Layout"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="vertical"
				android:visibility="gone"
				android:animateLayoutChanges="true">
				<com.synapse.social.studioasinc.animations.textview.TVeffects
					android:id="@+id/aiResponseTextView_2"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginTop="15dp"
					android:text="Hello"
					android:textSize="16sp"
					android:textColor="#000000"
					android:fontFamily="@font/product_sans" />
				<LinearLayout
					android:id="@+id/linear4"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="15dp"
					android:padding="12dp"
					android:orientation="vertical"
					android:background="@drawable/white_rounded_stroke"
					android:visibility="gone">
					<LinearLayout
						android:id="@+id/ruleLayout1"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="horizontal">
						<LinearLayout
							android:id="@+id/linear6"
							android:layout_width="wrap_content"
							android:layout_height="match_parent"
							android:padding="8dp"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/ic_rule_1"
								android:layout_width="25dp"
								android:layout_height="30dp"
								android:src="@drawable/star_shine_24px"
								android:scaleType="fitCenter" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear7"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:orientation="vertical"
							android:animateLayoutChanges="true">
							<com.synapse.social.studioasinc.animations.textview.TVeffects
								android:id="@+id/ruleTextView1"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="4dp"
								android:textSize="16sp"
								android:textColor="#000000"
								android:fontFamily="@font/product_sans" />
						</LinearLayout>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/ruleLayout2"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginTop="10dp"
						android:orientation="horizontal">
						<LinearLayout
							android:id="@+id/linear9"
							android:layout_width="wrap_content"
							android:layout_height="match_parent"
							android:padding="8dp"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/ic_rule_2"
								android:layout_width="25dp"
								android:layout_height="30dp"
								android:src="@drawable/ic_docs_48px"
								android:scaleType="fitCenter" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear10"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:orientation="vertical">
							<com.synapse.social.studioasinc.animations.textview.TVeffects
								android:id="@+id/ruleTextView2"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="4dp"
								android:text="You own the content you share, but you give Synapse permission to display and share it inside the app. Do not post anything illegal, offensive, or harmful. Spamming, impersonation, hate speech, or harassment of any kind is not allowed and may lead to account suspension."
								android:textSize="16sp"
								android:textColor="#000000"
								android:fontFamily="@font/product_sans" />
						</LinearLayout>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/ruleLayout3"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginTop="10dp"
						android:orientation="horizontal">
						<LinearLayout
							android:id="@+id/linear12"
							android:layout_width="wrap_content"
							android:layout_height="match_parent"
							android:padding="8dp"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/ic_rule_3"
								android:layout_width="25dp"
								android:layout_height="30dp"
								android:src="@drawable/ic_shield_lock"
								android:scaleType="fitCenter" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear13"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:orientation="vertical">
							<com.synapse.social.studioasinc.animations.textview.TVeffects
								android:id="@+id/ruleTextView3"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="4dp"
								android:text="We respect your privacy. Synapse collects only the minimum data needed to run and improve the app. We do not sell your information. By continuing to use Synapse, you accept future updates to these terms. For questions, contact our support team."
								android:textSize="16sp"
								android:textColor="#000000"
								android:fontFamily="@font/product_sans" />
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
				<Button
					android:id="@+id/finishButton"
					android:layout_width="match_parent"
					android:layout_height="60dp"
					android:layout_marginTop="20dp"
					android:padding="8dp"
					android:text="I agree"
					android:textSize="16sp"
					android:textColor="?colorSurface"
					style="@style/Widget.Material3.Button"
					android:fontFamily="@font/product_sans_bold" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/section3Layout"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="15dp"
				android:gravity="center_horizontal"
				android:orientation="vertical"
				android:visibility="gone"
				android:animateLayoutChanges="true">
				<LinearLayout
					android:id="@+id/emailLayout"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="16dp"
					android:orientation="vertical"
					android:background="@drawable/white_rounded_stroke"
					android:animateLayoutChanges="true">
					<LinearLayout
						android:id="@+id/linear18"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="horizontal"
						android:animateLayoutChanges="true">
						<LinearLayout
							android:id="@+id/linear19"
							android:layout_width="wrap_content"
							android:layout_height="match_parent"
							android:layout_marginRight="10dp"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical"
							android:visibility="gone"
							android:animateLayoutChanges="true">
							<LinearLayout
								android:id="@+id/linear21"
								android:layout_width="50dp"
								android:layout_height="50dp"
								android:gravity="center_horizontal|center_vertical"
								android:orientation="vertical"
								android:background="@drawable/circlelayout">
								<TextView
									android:id="@+id/textview1"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:text="@"
									android:textSize="20sp"
									android:textStyle="bold"
									android:textColor="?colorSurface" />
							</LinearLayout>
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear20"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:orientation="vertical">
							<TextView
								android:id="@+id/ask_for_email_tv"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="Give me your email address"
								android:textSize="16sp"
								android:textColor="#000000"
								android:fontFamily="@font/product_sans" />
							<LinearLayout
								android:id="@+id/linear22"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:layout_marginTop="5dp"
								android:background="@android:color/transparent"
								android:orientation="horizontal">
								<EditText
									android:id="@+id/email_et"
									android:layout_width="match_parent"
									android:layout_height="match_parent"
									android:padding="2dp"
									android:background="@android:color/transparent"
									android:gravity="center_vertical"
									android:textSize="18sp"
									android:textColor="#000000"
									android:hint="Enter your email"
									android:textColorHint="#9E9E9E"
									android:singleLine="true"
									android:fontFamily="@font/product_sans" />
							</LinearLayout>
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear17"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="10dp"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/passLayout"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="16dp"
						android:orientation="vertical"
						android:background="@drawable/white_rounded_stroke"
						android:animateLayoutChanges="true">
						<LinearLayout
							android:id="@+id/linear56"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:orientation="horizontal"
							android:animateLayoutChanges="true">
							<LinearLayout
								android:id="@+id/linear57"
								android:layout_width="wrap_content"
								android:layout_height="match_parent"
								android:layout_marginRight="10dp"
								android:gravity="center_horizontal|center_vertical"
								android:orientation="vertical"
								android:visibility="gone"
								android:animateLayoutChanges="true">
								<LinearLayout
									android:id="@+id/linear59"
									android:layout_width="50dp"
									android:layout_height="50dp"
									android:gravity="center_horizontal|center_vertical"
									android:orientation="vertical"
									android:background="@drawable/circlelayout">
									<TextView
										android:id="@+id/textview14"
										android:layout_width="wrap_content"
										android:layout_height="wrap_content"
										android:text="A"
										android:textSize="20sp"
										android:textStyle="bold"
										android:textColor="?colorSurface"
										android:fontFamily="@font/product_sans" />
								</LinearLayout>
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear58"
								android:layout_width="match_parent"
								android:layout_height="match_parent"
								android:orientation="vertical">
								<TextView
									android:id="@+id/askForPassTV"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:text="Now, your password"
									android:textSize="16sp"
									android:textColor="#000000"
									android:fontFamily="@font/product_sans" />
								<LinearLayout
									android:id="@+id/linear60"
									android:layout_width="match_parent"
									android:layout_height="wrap_content"
									android:layout_marginTop="5dp"
									android:background="@android:color/transparent"
									android:orientation="horizontal">
									<EditText
										android:id="@+id/pass_et"
										android:layout_width="match_parent"
										android:layout_height="match_parent"
										android:padding="2dp"
										android:background="@android:color/transparent"
										android:gravity="center_vertical"
										android:textSize="18sp"
										android:textColor="#000000"
										android:hint="Enter your password"
										android:textColorHint="#9E9E9E"
										android:singleLine="true"
										android:fontFamily="@font/product_sans" />
								</LinearLayout>
							</LinearLayout>
						</LinearLayout>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear24"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginTop="15dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<Button
							android:id="@+id/button1"
							android:layout_width="match_parent"
							android:layout_height="60dp"
							android:padding="8dp"
							android:text="Continue"
							android:textSize="16sp"
							android:textColor="?colorSurface"
							style="@style/Widget.Material3.Button"
							android:fontFamily="@font/product_sans_bold" />
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
