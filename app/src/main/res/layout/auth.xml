<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical"
	android:id="@+id/_main">
	<ScrollView
		android:id="@+id/vscroll1"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="@color/md_theme_background"
		android:scrollbars="none">
		<LinearLayout
			android:id="@+id/parentLayout"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:paddingLeft="@dimen/auth_padding_left"
			android:paddingTop="@dimen/auth_padding_top"
			android:paddingRight="@dimen/auth_padding_right"
			android:paddingBottom="@dimen/auth_padding_bottom"
			android:background="@android:color/transparent"
			android:orientation="vertical"
			android:animateLayoutChanges="true">
			<com.synapse.social.studioasinc.animations.textview.TVeffects
				android:id="@+id/aiNameTextView"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="@string/auth_hello"
				android:textSize="@dimen/auth_title_text_size"
				android:textStyle="bold"
				android:textColor="@color/md_theme_onBackground"
				android:fontFamily="@font/product_sans_bold" />
			<com.synapse.social.studioasinc.animations.textview.TVeffects
				android:id="@+id/aiResponseTextView_1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/auth_margin_top"
				android:textSize="@dimen/auth_response_text_size"
				android:textColor="@color/black"
				android:fontFamily="@font/product_sans" />
			<LinearLayout
				android:id="@+id/mainHiddenLayout"
				android:layout_width="match_parent"
				android:layout_height="@dimen/auth_hidden_layout_height"
				android:layout_marginTop="@dimen/auth_margin_top"
				android:gravity="center_horizontal"
				android:orientation="vertical"
				android:visibility="gone"
				android:animateLayoutChanges="true">
				<LinearLayout
					android:id="@+id/nameLayout"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="@dimen/auth_name_layout_padding"
					android:orientation="vertical"
					android:background="@drawable/white_rounded_stroke"
					android:animateLayoutChanges="true">
					<LinearLayout
						android:id="@+id/profileSeparatorLayout"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="horizontal"
						android:animateLayoutChanges="true">
						<LinearLayout
							android:id="@+id/profileHolderLayout"
							android:layout_width="wrap_content"
							android:layout_height="match_parent"
							android:layout_marginRight="@dimen/auth_profile_margin_right"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical"
							android:visibility="gone"
							android:animateLayoutChanges="true">
							<LinearLayout
								android:id="@+id/latterProfilePictureLayoutCircle"
								android:layout_width="@dimen/auth_profile_picture_size"
								android:layout_height="@dimen/auth_profile_picture_size"
								android:gravity="center_horizontal|center_vertical"
								android:orientation="vertical"
								android:background="@drawable/circlelayout">
								<TextView
									android:id="@+id/nameFirstLetterTextView"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:text="A"
									android:textSize="@dimen/auth_name_letter_text_size"
									android:textStyle="bold"
									android:textColor="?colorSurface" />
							</LinearLayout>
						</LinearLayout>
						<LinearLayout
							android:id="@+id/nameLayoutHolderLayout"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:orientation="vertical">
							<TextView
								android:id="@+id/userAutoResponseTextView_1"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="@string/auth_nice_to_meet_you"
								android:textSize="@dimen/auth_user_response_text_size"
								android:textColor="@color/black"
								android:fontFamily="@font/product_sans" />
							<LinearLayout
								android:id="@+id/EditTextContainerLayout"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:layout_marginTop="@dimen/auth_edit_text_margin_top"
								android:background="@android:color/transparent"
								android:orientation="horizontal">
								<EditText
									android:id="@+id/usernameEditText"
									android:layout_width="match_parent"
									android:layout_height="match_parent"
									android:padding="@dimen/auth_edit_text_padding"
									android:background="@android:color/transparent"
									android:gravity="center_vertical"
									android:textSize="@dimen/auth_response_text_size"
									android:textColor="@color/black"
									android:hint="@string/auth_enter_your_name"
									android:textColorHint="@color/gray"
									android:singleLine="true"
									android:fontFamily="@font/product_sans" />
							</LinearLayout>
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/animatorSupportLayout"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="@dimen/auth_animator_support_padding"
					android:orientation="vertical"
					android:layout_weight="1" />
				<LinearLayout
					android:id="@+id/linear2"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="@dimen/auth_profile_margin_right"
					android:padding="@dimen/auth_animator_support_padding"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/ageCheckboxLayout"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="horizontal"
						android:background="@drawable/rounded_stroke">
						<CheckBox
							android:id="@+id/confirmAgeCheckBox"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginLeft="@dimen/auth_checkbox_margin_left"
							android:layout_marginRight="@dimen/auth_checkbox_margin_right"
							android:background="@android:color/transparent"
							android:gravity="center_vertical"
							android:text="@string/age_agreement"
							android:textSize="@dimen/auth_checkbox_text_size"
							android:textStyle="bold"
							android:textColor="?colorOnSurface"
							android:fontFamily="@font/product_sans" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/continueBtnHolderLayout"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginTop="@dimen/auth_margin_top"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<Button
							android:id="@+id/continueButton"
							android:layout_width="match_parent"
							android:layout_height="@dimen/auth_continue_button_height"
							android:padding="@dimen/auth_animator_support_padding"
							android:text="@string/auth_continue"
							android:textSize="@dimen/auth_user_response_text_size"
							android:textColor="?colorSurface"
							style="@style/Widget.Material3.Button"
							android:fontFamily="@font/product_sans_bold" />
					</LinearLayout>
					<TextView
						android:id="@+id/infoTextView_1"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginTop="@dimen/auth_margin_top"
						android:gravity="center_horizontal|center_vertical"
						android:text="@string/auth_you_can_change_name_later"
						android:textSize="@dimen/auth_info_text_size"
						android:textColor="@color/md_theme_onBackground"
						android:fontFamily="@font/product_sans" />
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/section2Layout"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="vertical"
				android:visibility="gone"
				android:animateLayoutChanges="true">
				<com.synapse.social.studioasinc.animations.textview.TVeffects
					android:id="@+id/aiResponseTextView_2"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginTop="@dimen/auth_margin_top"
					android:text="@string/auth_hello_user"
					android:textSize="@dimen/auth_user_response_text_size"
					android:textColor="@color/black"
					android:fontFamily="@font/product_sans" />
				<LinearLayout
					android:id="@+id/linear4"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="@dimen/auth_margin_top"
					android:padding="@dimen/auth_checkbox_margin_left"
					android:orientation="vertical"
					android:background="@drawable/white_rounded_stroke"
					android:visibility="gone">
					<LinearLayout
						android:id="@+id/ruleLayout1"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="horizontal">
						<LinearLayout
							android:id="@+id/linear6"
							android:layout_width="wrap_content"
							android:layout_height="match_parent"
							android:padding="@dimen/auth_animator_support_padding"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/ic_rule_1"
								android:layout_width="@dimen/auth_rule_icon_width"
								android:layout_height="@dimen/auth_rule_icon_height"
								android:src="@drawable/star_shine_24px"
								android:scaleType="fitCenter" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear7"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:orientation="vertical"
							android:animateLayoutChanges="true">
							<com.synapse.social.studioasinc.animations.textview.TVeffects
								android:id="@+id/ruleTextView1"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="@dimen/auth_rule_text_padding"
								android:textSize="@dimen/auth_user_response_text_size"
								android:textColor="@color/black"
								android:fontFamily="@font/product_sans" />
						</LinearLayout>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/ruleLayout2"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginTop="@dimen/auth_profile_margin_right"
						android:orientation="horizontal">
						<LinearLayout
							android:id="@+id/linear9"
							android:layout_width="wrap_content"
							android:layout_height="match_parent"
							android:padding="@dimen/auth_animator_support_padding"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/ic_rule_2"
								android:layout_width="@dimen/auth_rule_icon_width"
								android:layout_height="@dimen/auth_rule_icon_height"
								android:src="@drawable/ic_docs_48px"
								android:scaleType="fitCenter" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear10"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:orientation="vertical">
							<com.synapse.social.studioasinc.animations.textview.TVeffects
								android:id="@+id/ruleTextView2"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="@dimen/auth_rule_text_padding"
								android:text="@string/auth_terms_of_service"
								android:textSize="@dimen/auth_user_response_text_size"
								android:textColor="@color/black"
								android:fontFamily="@font/product_sans" />
						</LinearLayout>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/ruleLayout3"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginTop="@dimen/auth_profile_margin_right"
						android:orientation="horizontal">
						<LinearLayout
							android:id="@+id/linear12"
							android:layout_width="wrap_content"
							android:layout_height="match_parent"
							android:padding="@dimen/auth_animator_support_padding"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/ic_rule_3"
								android:layout_width="@dimen/auth_rule_icon_width"
								android:layout_height="@dimen/auth_rule_icon_height"
								android:src="@drawable/ic_shield_lock"
								android:scaleType="fitCenter" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear13"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:orientation="vertical">
							<com.synapse.social.studioasinc.animations.textview.TVeffects
								android:id="@+id/ruleTextView3"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="@dimen/auth_rule_text_padding"
								android:text="@string/auth_privacy_policy"
								android:textSize="@dimen/auth_user_response_text_size"
								android:textColor="@color/black"
								android:fontFamily="@font/product_sans" />
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
				<Button
					android:id="@+id/finishButton"
					android:layout_width="match_parent"
					android:layout_height="@dimen/auth_continue_button_height"
					android:layout_marginTop="@dimen/auth_finish_button_margin_top"
					android:padding="@dimen/auth_animator_support_padding"
					android:text="@string/auth_i_agree"
					android:textSize="@dimen/auth_user_response_text_size"
					android:textColor="?colorSurface"
					style="@style/Widget.Material3.Button"
					android:fontFamily="@font/product_sans_bold" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/section3Layout"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/auth_margin_top"
				android:gravity="center_horizontal"
				android:orientation="vertical"
				android:visibility="gone"
				android:animateLayoutChanges="true">
				<LinearLayout
					android:id="@+id/emailLayout"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="@dimen/auth_name_layout_padding"
					android:orientation="vertical"
					android:background="@drawable/white_rounded_stroke"
					android:animateLayoutChanges="true">
					<LinearLayout
						android:id="@+id/linear18"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="horizontal"
						android:animateLayoutChanges="true">
						<LinearLayout
							android:id="@+id/linear19"
							android:layout_width="wrap_content"
							android:layout_height="match_parent"
							android:layout_marginRight="@dimen/auth_profile_margin_right"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical"
							android:visibility="gone"
							android:animateLayoutChanges="true">
							<LinearLayout
								android:id="@+id/linear21"
								android:layout_width="@dimen/auth_profile_picture_size"
								android:layout_height="@dimen/auth_profile_picture_size"
								android:gravity="center_horizontal|center_vertical"
								android:orientation="vertical"
								android:background="@drawable/circlelayout">
								<TextView
									android:id="@+id/textview1"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:text="@"
									android:textSize="@dimen/auth_name_letter_text_size"
									android:textStyle="bold"
									android:textColor="?colorSurface" />
							</LinearLayout>
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear20"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:orientation="vertical">
							<TextView
								android:id="@+id/ask_for_email_tv"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="@string/auth_give_email"
								android:textSize="@dimen/auth_user_response_text_size"
								android:textColor="@color/black"
								android:fontFamily="@font/product_sans" />
							<LinearLayout
								android:id="@+id/linear22"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:layout_marginTop="@dimen/auth_edit_text_margin_top"
								android:background="@android:color/transparent"
								android:orientation="horizontal">
								<EditText
									android:id="@+id/email_et"
									android:layout_width="match_parent"
									android:layout_height="match_parent"
									android:padding="@dimen/auth_edit_text_padding"
									android:background="@android:color/transparent"
									android:gravity="center_vertical"
									android:textSize="@dimen/auth_response_text_size"
									android:textColor="@color/black"
									android:hint="@string/auth_enter_email"
									android:textColorHint="@color/gray"
									android:singleLine="true"
									android:fontFamily="@font/product_sans" />
							</LinearLayout>
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear17"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="@dimen/auth_profile_margin_right"
					android:padding="@dimen/auth_animator_support_padding"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/passLayout"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="@dimen/auth_name_layout_padding"
						android:orientation="vertical"
						android:background="@drawable/white_rounded_stroke"
						android:animateLayoutChanges="true">
						<LinearLayout
							android:id="@+id/linear56"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:orientation="horizontal"
							android:animateLayoutChanges="true">
							<LinearLayout
								android:id="@+id/linear57"
								android:layout_width="wrap_content"
								android:layout_height="match_parent"
								android:layout_marginRight="@dimen/auth_profile_margin_right"
								android:gravity="center_horizontal|center_vertical"
								android:orientation="vertical"
								android:visibility="gone"
								android:animateLayoutChanges="true">
								<LinearLayout
									android:id="@+id/linear59"
									android:layout_width="@dimen/auth_profile_picture_size"
									android:layout_height="@dimen/auth_profile_picture_size"
									android:gravity="center_horizontal|center_vertical"
									android:orientation="vertical"
									android:background="@drawable/circlelayout">
									<TextView
										android:id="@+id/textview14"
										android:layout_width="wrap_content"
										android:layout_height="wrap_content"
										android:text="A"
										android:textSize="@dimen/auth_name_letter_text_size"
										android:textStyle="bold"
										android:textColor="?colorSurface"
										android:fontFamily="@font/product_sans" />
								</LinearLayout>
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear58"
								android:layout_width="match_parent"
								android:layout_height="match_parent"
								android:orientation="vertical">
								<TextView
									android:id="@+id/askForPassTV"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:text="@string/auth_your_password"
									android:textSize="@dimen/auth_user_response_text_size"
									android:textColor="@color/black"
									android:fontFamily="@font/product_sans" />
								<LinearLayout
									android:id="@+id/linear60"
									android:layout_width="match_parent"
									android:layout_height="wrap_content"
									android:layout_marginTop="@dimen/auth_edit_text_margin_top"
									android:background="@android:color/transparent"
									android:orientation="horizontal">
									<EditText
										android:id="@+id/pass_et"
										android:layout_width="match_parent"
										android:layout_height="match_parent"
										android:padding="@dimen/auth_edit_text_padding"
										android:background="@android:color/transparent"
										android:gravity="center_vertical"
										android:textSize="@dimen/auth_response_text_size"
										android:textColor="@color/black"
										android:hint="@string/auth_enter_password"
										android:textColorHint="@color/gray"
										android:singleLine="true"
										android:fontFamily="@font/product_sans" />
								</LinearLayout>
							</LinearLayout>
						</LinearLayout>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear24"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginTop="@dimen/auth_margin_top"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<Button
							android:id="@+id/button1"
							android:layout_width="match_parent"
							android:layout_height="@dimen/auth_continue_button_height"
							android:padding="@dimen/auth_animator_support_padding"
							android:text="@string/auth_continue"
							android:textSize="@dimen/auth_user_response_text_size"
							android:textColor="?colorSurface"
							style="@style/Widget.Material3.Button"
							android:fontFamily="@font/product_sans_bold" />
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
